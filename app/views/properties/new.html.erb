<div align="center"><h3>Add a new property</h3></div>

<%= form_for @property do |f| %>
	<%= f.label :address1 %>
	<%= f.text_field :address1 %>
	<%= f.label :address2 %>
	<%= f.text_field :address2 %>
	<%= f.label :city %>
	<%= f.text_field :city %>
	<%= f.label :state %>
	<%= f.text_field :state %>
	<%= f.label :zip %>
	<%= f.text_field :zip %>
	<%= f.label :propertytype %>
	<%= f.text_field :propertytype %>
	<%= f.submit "Create new property" %>
<% end %>