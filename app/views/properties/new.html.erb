<div align="center"><h3>Add a new property</h3></div>

<%= form_for @property do |f| %>
	<div>
		<%= f.label "Street Address" %>
		<%= f.text_field :address1 %>
	</div>

	<div>
		<%= f.label :city %>
		<%= f.text_field :city %>
	</div>

	<div>
		<%= f.label :state %>
		<%= f.text_field :state %>
	</div>

	<div>
		<%= f.label "Zip code" %>
		<%= f.text_field :zip, maxlength: 5 %>
	</div>

	<div>
		<%= f.label "Type of property" %>
		<%= f.text_field :propertytype %>
	</div>
	<%= f.submit "Create new property" %>
<% end %>